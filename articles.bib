@thesis{taoMianXiangYouXiShangChengDeFenBuShiTuiJianJiShuYanJiu2020,
  type = {硕士},
  title = {面向游戏商城的分布式推荐技术研究},
  author = {陶, 贵俊},
  date = {2020},
  institution = {{浙江大学}},
  url = {https://kns.cnki.net/KCMS/detail/detail.aspx?dbcode=CMFD&dbname=CMFDTEMP&filename=1021830607.nh&v=},
  abstract = {随着互联网的发展,网络游戏已成为人们日常娱乐的重要来源。游戏商城中物品成千上万,玩家很难快速找到适合自己的物品,因此个性化推荐系统已是网游中不可或缺的一部分。在解决特征矩阵稀疏问题时,相比SVD矩阵降维算法,混合协同过滤算法可有效避免核心数据丢失,但此算法需要大量的运算时间,个性化推荐延时较重,严重影响用户体验。本文通过分布式技术减少混合协同过滤算法的运算时间,详细分析了目前主流分布式算法出现的问题:一致性哈希算法无法保证任务分配的均匀性;均匀轮询算法无法根据服务间计算能力的不同及任务间复杂度的差异性进行动态合理分配任务;按服务器计算能力的差异性分配任务算法,无法根据任务间复杂度的差异性进行动态合理分配任务。为解决上述问题,本文提出一种基于任务完成情况动态调整的负载均衡策略,通过定时监测各服务本轮的任务完成数量,将其作为下一轮各服务分派任务的基数,此负载均衡策略可有效解决因服务器计算能力不同及任务间复杂度差异性带来的任务分配不合理问题,从而大幅度降低算法的整体运算时间。本文基于网游数据特性建立算法模型,将玩家行为分为玩家活动行为和玩家购买行为,对玩家活动行为做基于用户多维相似度协同过滤计算,解决购买行为特征矩阵稀疏问题;对玩家购买行为做基于物品的协同过滤计算,获得物品间的关联关系。本文算法在评测中展现出良好的扩展性,当物理机达到8台时,运算时间从单服务的24小时25分缩短到3小时47分。对比基于负载有界的一致性哈希、均匀轮询、服务器计算能力分类三种分布式算法,本文算法运算效率分别提升18\%、14.5\%、7\%。此外在测试环境对比SVD与Borda排序相结合的协同过滤算法,本文算法准确率提高7\%。},
  editora = {邵, 健 and 陈, 清华},
  editoratype = {collaborator},
  langid = {chinese},
  keywords = {collaborative filtering,game mall recommendation,recommendation algorithm,分布式推荐算法,协同过滤,推荐算法,游戏商城推荐 distributed recommendation algorithm},
  file = {D\:\\Austi\\Documents\\Zotero\\attachments\\2020\\陶\\面向游戏商城的分布式推荐技术研究\\陶_2020_面向游戏商城的分布式推荐技术研究.pdf}
}

